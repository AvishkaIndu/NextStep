{
  "name": "nextstep-setup",
  "version": "1.0.0",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "npm run dev",
    "install-all": "npm install && cd server && npm install",
    "build": "npm run build && cd server && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
